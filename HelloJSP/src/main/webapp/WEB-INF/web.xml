<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  
  <display-name>HelloJSP</display-name>
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>HelloServlet</servlet-name>
  	<servlet-class>myservlet.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HelloServlet</servlet-name>
  	<url-pattern>/HelloServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>NameServlet</servlet-name>
  	<servlet-class>myservlet.NameServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>NameServlet</servlet-name>
  	<url-pattern>/NameServlet</url-pattern>
  </servlet-mapping>
  
  <!-- 
  <servlet>
  	<servlet-name>CalcServlet</servlet-name>
  	<servlet-class>myservlet.CalcServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CalcServlet</servlet-name>
  	<url-pattern>/CalcServlet</url-pattern>
  </servlet-mapping>
  -->
  
</web-app>